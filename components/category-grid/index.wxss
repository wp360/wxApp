/* components/category-grid/index.wxss */
.container {
  width: 100%;
  height: 320rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  background-color: white;
}

.inner-container {
  width: 730rpx;
  height: 300rpx;
  border: 1px dashed #DEEBE6;
}

.grid-item {
  width: 200rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.img {
  width: 60rpx;
  height: 60rpx;
}